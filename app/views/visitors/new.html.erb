<% content_for :title do %>Rails Bootstrap<% end %>
<% content_for :description do %>Rails Bootstrap Example<% end %>
<div id="myCarousel" class="carousel slide">
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner">
    <div class="item active">
      <img src="http://www.fillmurray.com/1170/600">
      <div class="carousel-caption text-center">
        <h4>Bill Murray</h4>
      </div>
    </div>
    <div class="item">
      <img src="http://www.nicenicejpg.com/1170/600">
      <div class="carousel-caption text-center">
        <h4>Vanilla Ice</h4>
      </div>
    </div>
    <div class="item">
      <img src="http://www.placecage.com/c/1170/600">
      <div class="carousel-caption text-center">
        <h4>Nicolas Cage</h4>
      </div>
    </div>
  </div>
  <a class="left carousel-control" href="#myCarousel" data-slide="prev">&lsaquo;</a>
  <a class="right carousel-control" href="#myCarousel" data-slide="next">&rsaquo;</a>
</div>